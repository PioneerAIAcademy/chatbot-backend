# Authentication
# Required for API endpoints authentication
API_SECRET=your-secret-key-here

# OpenAI Configuration
# Required for OpenAI API access
OPENAI_API_KEY=your-openai-api-key-here

# Provider Configuration
# Optional: Specify which LLM provider to use (defaults to 'openai')
PROVIDER_NAME=openai

# DynamoDB Configuration for local development
DYNAMODB_URL=http://localhost:8000

# Log level
LOG_LEVEL=DEBUG

# Optional: Specify the path to the S3 bucket for data storage
DATA_S3_BUCKET=s3://your-bucket-name/path
